{
  "short_name": "DLNM",
  "name": "Delay No More",
  "version": "1.0",
  "manifest_version": 2,
  "background" : {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Delay No More"
  },
  "permissions": [
    "tabs",
    "notifications"
  ],

  "key": "",

  "content_security_policy":"script-src 'self' https://apis.google.com https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}